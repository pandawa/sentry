{
    "name": "pandawa/sentry",
    "description": "Sentry support for pandawa",
    "keywords": [
        "laravel",
        "pandawa",
        "sentry"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Iqbal Maulana",
            "email": "iq.bluejack@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "pandawa/pandawa": "^4.0",
        "sentry/sentry-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Pandawa\\Sentry\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Pandawa\\Sentry\\PandawaSentryModule"
            ]
        }
    }
}
